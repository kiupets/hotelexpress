{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "runtime": "@vercel/node@3.0.0",
        "memory": 1024,
        "maxDuration": 10
      },
      "index.js": {
        "runtime": "@vercel/node@3.0.0",
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }